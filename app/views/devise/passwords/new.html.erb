<div class="w-380 h-92 bg-[rgb(237,238,240)] flex justify-center items-center">
  <div class="w-130 h-70 bg-[rgb(255,255,255)]">
    <div class="w-130 h-15 bg-[rgb(255,255,255)] ">
      <div class="w-130 h-15 bg-[rgb(255,255,255)] ">
        <div class="w-50 h-15 bg-[rgb(255,255,255)] flex justify-center items-center ml-3">
          <div class="w-50 h-15 bg-[rgb(255,255,255)] flex items-center">
            <div class="h-8 bg-[rgb(255,255,255)] text-2xl font-500 font-semibold">
              Find your account
            </div>
          </div>
        </div>
      </div>
      <div class="w-130 h-0.4  bg-[rgb(255,255,255)] ">
        <hr class="w-130 h-0.4 size-10% bg-[rgb(242,242,242)] text-[rgb(242,242,242)]"/>
      </div>
      <!--div class="w-140 h-5 bg-green-600"--> 
        <!--hr/-->
      <!--/div-->
    </div>
    <div class="w-130 h-52 bg-[rgb(255,255,255)]">
      <div class="w-130 h-14 bg-[rgb(255,255,255)] flex justify-center items-center mt-3">
        <div class="w-130 h-14 bg-[rgb(255,255,255)] text-lg flex items-center">
          <div class="w-130 h-14 bg-[rgb(255,255,255)] flex justify-center">
            <div class="w-120 bg-[rgb(255,255,255))]">
              <div class="w-120 h-7 bg-[rgb(255,255,255)] text-lg">
                Please enter your email address or mobile number to search
             </div>
              <div class="w-120 h-8 bg-[rgb(255,255,255)] text-lg">
                for your account.
              </div>
            </div>
          </div>
        </div>
      </div>
      <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
   
        <div>
          <div class="field flex justify-center items-center h-21">
            <!--%= f.label :email %><br /-->
            <div class="w-120 h-14">
              <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"w-120 h-14 border border-[rgb(205,209,214)] rounded-lg" %>
            </div>
            
          </div>
          <hr class="w-130 h-0.4 size-10% bg-[rgb(242,242,242)] text-[rgb(242,242,242)]"/>
        </div>
      <%= render "devise/shared/error_messages", resource: resource %>
        <div class="actions flex justify-end  h-17">
          <div class="h-17 flex items-center"> 
            <div class="flex justify-around w-60 h-10 ">
              <div>
                <button class="bg-[rgb(228,230,235)] w-30 h-10 rounded-lg font-[600]">
                   <%= link_to "Cancel", new_session_path(resource_name),class:"text-base font-bold text-[rgb(75,79,86)]" %>
                </button>
                
              </div> 
              <div class="w-25 h-10 bg-[rgb(255,255,255)] text-white">
                <button class="rounded-lg w-25 h-10 bg-[rgb(8,102,255)]  text-base font-[600] rounded-md"><%= f.submit "Search" %></button>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
 </div>
<!--%= render "devise/shared/links" %-->
</div>
